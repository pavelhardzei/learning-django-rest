ken'])\n    }\n\n    return (\n        <div className=\"App\">\n            <div className=\"row\">\n                <div className=\"col\">\n                    <h1>React and Django</h1>\n                </div>\n                <div className=\"col\">\n                    <button onClick={articleForm} className=\"btn btn-primary\">Insert Article</button>\n                </div>\n                <div className=\"col\">\n                    <button onClick={logoutBtn} className=\"btn btn-primary\">Logout</button>\n                </div>\n            </div>\n            <ArticleList articles={articles} editBtn={editBtn} deleteBtn={deleteBtn}/>\n            {editArticle ? <Form article={editArticle} updatedInformation={updatedInformation}\n                                 insertedInformation={insertedInformation}/> : null}\n        </div>\n    );\n}\n\nexport default App;\n","/home/pavelhardzei/Studying/projects/front_django/src/reportWebVitals.js",[],"/home/pavelhardzei/Studying/projects/front_django/src/components/ArticleList.js",[],"/home/pavelhardzei/Studying/projects/front_django/src/components/Form.js",[],"/home/pavelhardzei/Studying/projects/front_django/src/components/APIService.js",[],"/home/pavelhardzei/Studying/projects/front_django/src/components/Login.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":12,"column":19,"nodeType":"41","messageId":"42","endLine":12,"endColumn":27},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"no-unused-vars","'setToken' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]